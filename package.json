{
  "name": "eslint-plugin-react-prefer-function-component",
  "description": "ESLint lint rule to enforce function components in React",
  "license": "MIT",
  "author": "Tate <tatethurston@gmail.com>",
  "homepage": "https://github.com/tatethurston/eslint-plugin-react-prefer-function-component#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tatethurston/eslint-plugin-react-prefer-function-component.git"
  },
  "bugs": {
    "url": "https://github.com/tatethurston/eslint-plugin-react-prefer-function-component/issues"
  },
  "version": "0.0.7",
  "main": "dist/index.js",
  "files": [
    "dist/prefer-function-component/index.js"
  ],
  "scripts": {
    "build": "yarn clean && yarn tsc",
    "build:watch": "yarn build --watch",
    "clean": "rm -rf dist/*",
    "lint:fix": "yarn lint:fix:md && yarn lint:fix:package && yarn lint:fix:ts && prettier --write ./src && yarn typecheck",
    "lint:fix:md": "prettier --write *.md",
    "lint:fix:package": "prettier-package-json --write ./package.json",
    "lint:fix:ts": "eslint --fix './src/**/*.ts'",
    "test": "yarn build && jest --testTimeout 5000 --rootDir dist",
    "typecheck": "yarn tsc --noEmit",
    "typecheck:watch": "yarn typecheck --watch",
    "version": "yarn run build && git add -A package.json",
    "postversion": "git push && git push --tags"
  },
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "eslint-plugin-react": "^7.23.2"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.9",
    "@types/estree": "^0.0.47",
    "@types/jest": "^26.0.15",
    "@types/node": "^13.13.5",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^8.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.1",
    "prettier-package-json": "^2.1.3",
    "typescript": "^4.1.3"
  },
  "keywords": [
    "class component",
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "function component",
    "functional component",
    "no class",
    "react"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix"
    }
  }
}
